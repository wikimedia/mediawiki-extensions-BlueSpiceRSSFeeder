{
	"name": "RSSFeeder",
	"license-name": "GPL-3.0",
	"version": "3.0.0-alpha",
	"url": "https://help.bluespice.com/index.php/RSSFeeder",
	"author": "Sebastian Ulbricht",
	"descriptionmsg": "bs-rssfeeder-desc",
	"type": "bluespice",
	"bsgExtensions": {
		"RSSFeeder": {
			"className": "RSSFeeder",
			"extPath": "/BlueSpiceExtensions/RSSFeeder"
		}
	},
	"SpecialPages": {
		"RSSFeeder": "SpecialRSSFeeder"
	},
